FROM marccote19/textworld:v1.2
RUN apt update
RUN apt-get update
RUN apt install python3.7 -y
RUN  python3.7 -m pip -q install pip --upgrade

RUN python3.7 -m pip uninstall textworld -y
RUN python3.7 -m pip uninstall gym tqdm numpy docker spacy textworld -y
RUN python3.7 -m pip install spacy==2.2.4
RUN python3.7 -m spacy download en
RUN python3.7 -m pip install textworld==1.1.1 docker
RUN python3.7 -m pip install torch==1.9.0+cu102 torchvision==0.10.0+cu102 torchaudio===0.9.0 -f https://download.pytorch.org/whl/torch_stable.html

